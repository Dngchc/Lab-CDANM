check_memory = .bash_history : CONTAINS : valgrind --leak-check=full ./library_manager
check_error = .bash_history : CONTAINS : valgrind --track-origins=yes ./library_manager
check_operation = .bash_history : CONTAINS : valgrind --leak-check=full --show-leak-kinds=all ./library_manager
check_memory_release = .bash_history : CONTAINS : valgrind --track-origins=yes --show-reachable=yes ./library_manager 
check_read_write = .bash_history : CONTAINS : valgrind --undef-value-errors=yes ./library_manager
check_result = *.stdout : CONTAINS : *ERROR SUMMARY: 0 errors from 0 contexts*
